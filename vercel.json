{
    "version": 2,
    "functions": {
        "api": {
            "runtime": "python3.11"
        }
    },
    "builds": [
    {
        "src": "./wsgi.py",
        "use": "@vercel/python"
    }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
